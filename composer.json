{
    "name": "dfritsch/web",
    "version": "0.0.1",
    "description": "Webity base web framework, based on Joomla Framework",
    "keywords": ["web","framework"],
    "homepage": "http://www.makethewebwork.com",
    "license": "All Rights Reserved",
    "require": {
        "php": ">=5.3.10",
        "psr/log": "~1.0",
        "bshaffer/oauth2-server-php": "v1.3",
        "joomla/framework": "dev-formupgrade",
        "league/plates": "2.*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:dfritsch/joomla-framework.git"
        }
    ],
    "require-dev": {
         "symfony/yaml": "~2.0"
    },
    "suggest": {
        "symfony/yaml": "Install symfony/yaml 2.* if you require YAML support."
    },
    "autoload": {
        "psr-0": {
            "Psr": "vendor/psr/cache",
            "Webity": "src/"
        }
    },
    "minimum-stability": "beta"
}
